{
  "app": {
    "name": "Insight Media Generator",
    "version": "2.0.0",
    "description": "Image and Audio Media Generation Tool"
  },
  "stable_diffusion": {
    "api_url": "http://127.0.0.1:7860/sdapi/v1/txt2img",
    "models_path": "C:/HarmonicInsight/stable-diffusion-webui/models/Stable-diffusion",
    "lora_path": "C:/HarmonicInsight/stable-diffusion-webui/models/Lora",
    "output_path": "C:/HarmonicInsight/stable-diffusion-webui/outputs/txt2img-images"
  },
  "voicevox": {
    "api_url": "http://127.0.0.1:50021",
    "auto_discover": true,
    "output_path": "./data/audio"
  },
  "data": {
    "json_upload_dir": "./data/json_files",
    "database_file": "./data/insight_media.db"
  },
  "defaults": {
    "model": "dreamshaper_8.safetensors",
    "sampler": "DPM++ 2M Karras",
    "steps": 30,
    "cfg_scale": 6,
    "width": 768,
    "height": 768,
    "lora_weight": 0.8,
    "speaker_id": 3
  },
  "ApiSecurity": {
    "ApiKey": "",
    "RequestTimeoutSeconds": 300,
    "AllowedOrigins": []
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5100"
      }
    }
  }
}
